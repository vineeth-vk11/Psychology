<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.psychology">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Psychology">
        <activity
            android:name=".OnboardingUI.InitialVideoActivity"
            android:exported="true" />
        <activity
            android:name=".OnboardingUI.InitialMessageActivity"
            android:exported="true" />
        <activity
            android:name=".AccountUI.ContactUsActivity"
            android:exported="true" />
        <activity
            android:name=".AccountUI.KnowMoreActivity"
            android:exported="true" />
        <activity android:name=".AccountUI.EditAccountActivity" />
        <activity android:name=".StatsUI.PaStatsActivity" />
        <activity android:name=".StatsUI.MrStatsActivity" />
        <activity android:name=".StatsUI.ErtStatsActivity" />
        <activity android:name=".EntriesUI.PAEntryActivity" />
        <activity android:name=".EntriesUI.MREntryActivity" />
        <activity android:name=".EntriesUI.ERTEntryActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginUI.LoginMainActivity" />
        <activity android:name=".LoginUI.LoginOtpActivity" />
        <activity android:name=".LoginUI.LoginDetailsCollectionActivity" />
        <activity
            android:name=".OnboardingUI.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <receiver android:name=".Notification.ReminderBroadcast"/>
    </application>

</manifest>